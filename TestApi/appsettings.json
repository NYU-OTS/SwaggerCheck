{
  "Serilog": {
    "Using": ["Serilog.Sinks.Elasticsearch"],
    "MinimumLevel": "Information",
    "WriteTo": [
      {
        "Name": "Elasticsearch",
        "Args": {
          "nodeUris": "",
          "indexFormat": "ots-api-{0:yyyy-MM}"
        }
      }
    ],
    "Enrich": ["FromLogContext", "WithMachineName"],
    "Properties": {
      "Application": "TestApi"
    }
  }
}
